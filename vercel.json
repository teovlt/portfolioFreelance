{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "teovillet.fr"
        },
        {
          "type": "scheme",
          "value": "http"
        }
      ],
      "destination": "https://teovillet.fr/$1",
      "permanent": true
    }
  ]
}
